'tests' stamp: 'JanKorbel 1/3/2018 21:46'!testRemoveFoodstuff	| foodstore c | 	foodstore := OOP_Model_Foodstore singleton .	foodstore cleanUp .	foodstore addFoodstuffBestBefore: 'cukr'.	self assert: (foodstore foodstuffs size) equals: 1.	c := foodstore foodstuffs first .	foodstore addFoodstuffUseBy: 'jogurt'.	self assert: (foodstore foodstuffs size) equals: 2.	foodstore removeFoodstuff: c.	self assert: (foodstore foodstuffs size) equals: 1.	self assert: (foodstore foodstuffs first name) = 'jogurt'.	! !TestCase subclass: #OOP_Model_FoodstuffBatchTestCase	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: '_FIT-OOP-Spiz-Tests'!!OOP_Model_FoodstuffBatchTestCase methodsFor: 'tests' stamp: 'JanKorbel 1/7/2018 01:23'!testAccessors	| batch |	batch := OOP_Model_FoodstuffBatch new.	batch batchId: 'asdf-2018'.	batch itemCount: 20.	batch unitPrice: 30.	batch unitWeight: 0.2.	batch useBy: '2018-01-05'.	self assert: batch batchId = 'asdf-2018'.	self assert: batch itemCount equals: 20.	self assert: batch unitPrice equals: 30.	self assert: batch unitWeight equals: 0.2.	self assert: batch totalPrice equals: 600.	self assert: batch totalWeight equals: 4.	self assert: batch useBy equals: '2018-01-05'.! !TestCase subclass: #OOP_Model_FoodstuffBestBeforeTestCase	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: '_FIT-OOP-Spiz-Tests'!!OOP_Model_FoodstuffBestBeforeTestCase methodsFor: 'tests' stamp: 'JanKorbel 1/7/2018 01:35'!testPrice	| batch fs |	fs := OOP_Model_FoodstuffBestBefore new.	batch := OOP_Model_FoodstuffBatch new.	batch itemCount: 5 .	batch unitPrice: 15.	fs addBatch: batch .	self assert: fs totalPrice equals: 75.	batch := OOP_Model_FoodstuffBatch new.	batch itemCount: 6 .	batch unitPrice: 25.	fs addBatch: batch .	self assert: fs totalPrice equals: 225.! !!OOP_Model_FoodstuffBestBeforeTestCase methodsFor: 'tests' stamp: 'JanKorbel 1/7/2018 00:45'!testAddBatch	| batch fs |	fs := OOP_Model_FoodstuffBestBefore new.	fs name: 'fazole'.	batch := OOP_Model_FoodstuffBatch new.	batch batchId: 'ABC-2016'.	batch itemCount: 6.	batch unitPrice: 15.9 .	batch unitWeight: 0.5 .	self assert: fs batches size equals: 0.	fs addBatch: batch.	self assert: fs batches size equals: 1.				! !!OOP_Model_FoodstuffBestBeforeTestCase methodsFor: 'tests' stamp: 'JanKorbel 1/7/2018 01:40'!testWeight	| batch fs |	fs := OOP_Model_FoodstuffBestBefore new.	batch := OOP_Model_FoodstuffBatch new.	batch itemCount: 10 .	batch unitWeight: 0.5.	fs addBatch: batch .	self assert: fs totalWeight equals: 5.	batch := OOP_Model_FoodstuffBatch new.	batch itemCount: 6 .	batch unitWeight: 1.	fs addBatch: batch .	self assert: fs totalWeight equals: 11.! !!OOP_Model_FoodstuffBestBeforeTestCase methodsFor: 'tests' stamp: 'JanKorbel 1/3/2018 01:12'!testName	| fs |	fs := OOP_Model_FoodstuffBestBefore new.	fs name: 'fazole'.	self assert: fs name equals: 'fazole'.! !!OOP_Model_FoodstuffBestBeforeTestCase methodsFor: 'tests' stamp: 'JanKorbel 1/7/2018 00:52'!testInitialize	| fs |	fs := OOP_Model_FoodstuffBestBefore new.	self assert: (fs batches) size = 0.! !